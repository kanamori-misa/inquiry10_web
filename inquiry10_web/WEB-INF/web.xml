	</servlet-mapping>

	<servlet>
		<servlet-name>searchMapping</servlet-name>
		<servlet-class>servlet.SearchServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>searchMapping</servlet-name>
		<url-pattern>/search</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>bookmarkMapping</servlet-name>
		<servlet-class>servlet.BookMarkServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>bookmarkMapping</servlet-name>
		<url-pattern>/bookmark</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>userinsertMapping</servlet-name>
